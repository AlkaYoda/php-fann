<?xml version="1.0"?>
<package xmlns="http://pear.php.net/dtd/package-2.0"
		 xmlns:tasks="http://pear.php.net/dtd/tasks-1.0"
		 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.0"
		 xsi:schemaLocation="http://pear.php.net/dtd/tasks-1.0
							 http://pear.php.net/dtd/tasks-1.0.xsd
							 http://pear.php.net/dtd/package-2.0
							 http://pear.php.net/dtd/package-2.0.xsd">
  <name>fann</name>
  <channel>pecl.php.net</channel>
  <summary>Binding for FANN (Fast Artificial Neural Network) Library</summary>
  <description>This package provides PHP binding for FANN (Fast Artificial Neural Network) Library</description>
  <lead>
	<name>Jakub Zelenka</name>
	<user>bukka</user>
	<email>jakub.php@gmail.com</email>
	<active>yes</active>
  </lead>
  <lead>
	<name>Evan Nemerson</name>
	<user>enemerson</user>
	<email>evan@coeus-group.com</email>
	<active>no</active>
  </lead>

  <date>2013-08-02</date>
  <version>
	<release>1.0.0</release>
	<api>1.0.0</api>
  </version>
  <stability>
	<release>stable</release>
	<api>stable</api>
  </stability>
  <license uri="http://www.php.net/license">PHP</license>
  <notes><![CDATA[
  * New functional API supporting libfann version 2.1 and 2.2.
  * More than 140 new functions.
  * Many new algorithms and options for neural networks.
  * Incompatible with previous version (rewritten from scratch).
  ]]></notes>
  <contents>
	<dir name="/">
	  <file role="doc" name="CREDITS"/>
      <file role="doc" name="LICENSE"/>
      <file role="doc" name="README.md"/>
      <file role="doc" name="TODO.txt"/>
	  <file role="src" name="config.m4" />
	  <file role="src" name="config.w32" />
	  <file role="src" name="php_fann.h"/>
	  <file role="src" name="fann_connection.c"/>
	  <file role="src" name="fann.c"/>
	  <dir name="examples">
		<file role="doc" name="simple_test.php"/>
		<file role="doc" name="simple_train.php"/>
		<file role="doc" name="xor.data"/>
	  </dir>
	</dir>
  </contents>
  <dependencies>
    <required>
      <php>
        <min>5.2.0</min>
      </php>
      <pearinstaller>
        <min>1.4.0a1</min>
      </pearinstaller>
    </required>
  </dependencies>
  <providesextension>fann</providesextension>
  <extsrcrelease/>
	
</package>
